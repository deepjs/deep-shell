#!/usr/bin/env node
/**
 * @author Gilles Coomans <gilles.coomans@gmail.com>
 */
require("deep-shell");
deep.sh().log("dpsh").pwd();
deep.Promise.context.cwd = __dirname;

deep.sh.Protocol("sh");
deep.fs.Protocol("fs");

deep.sh.verbose = true;

var repl = require("repl")
.start({
	prompt: "> ",
	input: process.stdin,
	output: process.stdout
});